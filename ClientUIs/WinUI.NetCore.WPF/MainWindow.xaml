<Window x:Class="WinUI.NetCore.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinUI.NetCore.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button x:Name="ButtonLoadData" Click="ButtonLoadData_OnClick" Content="Click To Load Data" />
            <DataGrid x:Name="DataGridMain" AutoGenerateColumns="True" />
        </StackPanel>
    </Grid>
</Window>
